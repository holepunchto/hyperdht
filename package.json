{
  "name": "@hyperswarm/dht",
  "version": "3.0.1",
  "description": "The DHT backing the HyperSwarm stack",
  "main": "index.js",
  "dependencies": {
    "@hyperswarm/hypersign": "^2.0.0",
    "dht-rpc": "^4.1.6",
    "end-of-stream": "^1.4.1",
    "hashlru": "^2.3.0",
    "ipv4-peers": "^2.0.0",
    "protocol-buffers-encodings": "^1.1.0",
    "record-cache": "^1.1.0",
    "sodium-universal": "^2.0.0"
  },
  "devDependencies": {
    "get-port": "^5.0.0",
    "nonsynchronous": "^1.0.1",
    "protocol-buffers": "^4.1.0",
    "standard": "^13.1.0",
    "tap": "^14.5.0"
  },
  "bin": {
    "hyperswarm-dht": "./bin.js"
  },
  "scripts": {
    "test": "tap  -R classic test/*.test.js  && standard --fix",
    "cov": "tap -R classic --100 --coverage-report=html test/*.test.js",
    "ci": "standard && npm test",
    "protobuf": "protocol-buffers schema.proto -o messages.js"
  },
  "nyc": {
    "exclude": [
      "messages.js",
      "**/test/**"
    ]
  },
  "repository": {
    "type": "git",
